package com.example.myapplication;
import androidx.annotation.NonNull;
import androidx.room.ColumnInfo;
import androidx.room.Entity;
import androidx.room.PrimaryKey;

@Entity(tableName = "ocurrence")
public class Ocurrence {
 @PrimaryKey(autoGenerate = true)
 @NonNull
 @ColumnInfo(name = "ocurrenceId")
 private int id;
 @NonNull
 @ColumnInfo(name = "ocurrenceCreationDate")
 private String creationDate;
 @NonNull
 @ColumnInfo(name = "ocurrenceDescription")
 private String description;
 @NonNull
 @ColumnInfo(name = "ocurrenceForwarding")
 private String Forwarding;
 @NonNull
 @ColumnInfo(name = "ocurrenceGrade")
 private int grade;
 public int getId() {
 return this.id;
 }
 public void setId(int id) {
 this.id = id;
 }
 public String getCreationDate() {
 return this.creationDate;
 }
 public void setCreationDate(String creationDate) {
 this.creationDate = creationDate;
 }

@NonNull
    public String getDescription() {
        return description;
    }

    public void setDescription(@NonNull String description) {
        this.description = description;
    }

    @NonNull
    public String getForwarding() {
        return Forwarding;
    }

    public void setForwarding(@NonNull String forwarding) {
        Forwarding = forwarding;
    }

    public int getGrade() {
        return grade;
    }

    public void setGrade(int grade) {
        this.grade = grade;
    }
 
 
    public Ocurrence(@NonNull String creationDate, @NonNull String description, @NonNull String forwarding, int grade) {
        this.creationDate = creationDate;
        this.description = description;
        Forwarding = forwarding;
        this.grade = grade;
    }
 
 }
 
